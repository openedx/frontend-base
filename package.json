{
	"name": "@openedx/frontend-base",
	"version": "1.0.0",
	"description": "Build tools, setup and config for frontend apps",
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"/dist",
		"/config",
		"/cli"
	],
	"bin": {
		"openedx": "dist/bin/openedx.js",
		"intl-imports.js": "dist/cli/scripts/intl-imports.js",
		"transifex-utils.js": "dist/cli/scripts/transifex-utils.js"
	},
	"scripts": {
		"build": "tsc",
		"build:watch": "tsc --watch",
		"clean": "rm -rf dist",
		"docs": "jsdoc -c jsdoc.json",
		"docs:watch": "nodemon -w runtime -w docs/template -w README.md -e js,jsx,ts,tsx --exec npm run docs",
		"lint": "eslint .",
		"test": "npm run test:cli; npm run test:app; npm run test:runtime",
		"test:app": "npm --prefix ./test-app i; npm --prefix ./test-app run lint; npm --prefix ./test-app run build",
		"test:cli": "jest cli --config cli/jest.config.js --no-cache",
		"test:runtime": "jest runtime --config runtime/jest.config.js --no-cache"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/openedx/frontend-base.git"
	},
	"keywords": [],
	"author": "Open edX Community",
	"license": "AGPL-3.0",
	"bugs": {
		"url": "https://github.com/openedx/frontend-base/issues"
	},
	"sideEffects": false,
	"homepage": "https://github.com/openedx/frontend-base#readme",
